{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titel": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Titel",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "BtnText": {
      "type": "string",
      "required": true
    },
    "EventSneakCover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "PageCover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Event",
        "Work-Shop"
      ],
      "required": true,
      "default": "Event"
    },
    "PageBtnText": {
      "type": "string"
    },
    "PageBtnLink": {
      "type": "string"
    },
    "PageDescription": {
      "type": "richtext",
      "required": true
    }
  }
}
